[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Michael Faille (mikefaille)
[access "refs/for/refs/*"]
	read = group user/Michael Faille (mikefaille)
	push = group user/Michael Faille (mikefaille)
	label-Code-Review = -2..+2 group user/Michael Faille (mikefaille)
	label-Verified = -1..+1 group user/Michael Faille (mikefaille)
	rebase = group user/Michael Faille (mikefaille)
	removeReviewer = group user/Michael Faille (mikefaille)
	submit = group user/Michael Faille (mikefaille)
[access "refs/heads/*"]
	read = group user/Michael Faille (mikefaille)
	create = group user/Michael Faille (mikefaille)
	push = +force group user/Michael Faille (mikefaille)
	pushMerge = group user/Michael Faille (mikefaille)
[access "refs/tags/*"]
	push = group user/Michael Faille (mikefaille)
	push = +force group user/Michael Faille (mikefaille)
